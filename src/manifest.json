{
  "name": "Pomotasking",
  "background": {
    "page": "background.html"
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "browser-action-icon.png"
  },
  "icons": {
    "128": "pomotodo-128.png"
  },
  "permissions": ["notifications"],
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
